{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 3,
    "max": 20
  },
  "postgres": "postgres://postgres:@localhost:5432/travelpack",
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local",
      "Facebook Strategy"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://travelpack.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "service": "users",
      "usernameField": "username",
      "passwordField": "password",
      "session": "true" 
    }
  }
}
